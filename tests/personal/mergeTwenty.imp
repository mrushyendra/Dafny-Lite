program MergeTwenty
  pre forall i j, (0 <= i && j > i && j < 5) ==> (a[i] <= a[j])
  pre forall i j, (0 <= i && j > i && j < 5) ==> (b[i] <= b[j])
  post forall i j, (0 <= i && j > i && j < cIdx) ==> (c[i] <= c[j])
is
  aIdx := 0;
  bIdx := 0;
  cIdx := 0;
  while (cIdx < 5)
    inv forall i j, (0 <= i && j > i && j < cIdx) ==> (c[i] <= c[j])
    inv aIdx + bIdx = cIdx
    inv cIdx >= 0 && cIdx <= 5
    inv aIdx >= 0 && aIdx <= 5
    inv bIdx >= 0 && bIdx <= 5
  do
    if a[aIdx] <= b[bIdx] then
      c[cIdx] := a[aIdx];
      aIdx := aIdx + 1;
    else
      c[cIdx] := b[bIdx];
      bIdx := bIdx + 1;
    end
    cIdx := cIdx + 1;
  end
end
