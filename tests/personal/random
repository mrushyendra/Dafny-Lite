program MergeTwenty
  pre forall i j, (0 <= i && j > i && j < 20) ==> (a[i] <= a[j])
  pre forall i j, (0 <= i && j > i && j < 20) ==> (b[i] <= b[j])
  post forall i j, (0 <= i && j > i && j < 20) ==> (c[i] <= c[j])
is
  aIdx := 0;
  bIdx := 0;
  cIdx := 0;
  while (cIdx < 20)
    inv forall i j, (0 <= i && j > i && j < cIdx) ==> (c[i] <= c[j])
  do
    if a[aIdx] <= b[bIdx] then
      c[cIdx] = a[aIdx];
      aIdx = aIdx + 1;
    else
      c[cIdx] = b[bIdx];
      bIdx = bIdx + 1;
    end
    cIdx = cIdx + 1;
  end
end
